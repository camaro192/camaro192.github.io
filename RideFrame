<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="refresh" content="0; url=rideframe://oauth/callback?code=REPLACE_CODE_HERE" />
    <script>
      // Optionally parse the code param and redirect accordingly
      const params = new URLSearchParams(window.location.search);
      const code = params.get('code');
      if (code) {
        window.location.href = `rideframe://oauth/callback?code=${code}`;
      }
    </script>
</head>
<body>
    Redirecting...
</body>
</html>
